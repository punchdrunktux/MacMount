<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>CFBundleDevelopmentRegion</key>
    <string>$(DEVELOPMENT_LANGUAGE)</string>
    <key>CFBundleDisplayName</key>
    <string>MacMount</string>
    <key>CFBundleExecutable</key>
    <string>$(EXECUTABLE_NAME)</string>
    <key>CFBundleIconFile</key>
    <string>AppIcon</string>
    <key>CFBundleIdentifier</key>
    <string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
    <key>CFBundleInfoDictionaryVersion</key>
    <string>6.0</string>
    <key>CFBundleName</key>
    <string>$(PRODUCT_NAME)</string>
    <key>CFBundlePackageType</key>
    <string>$(PRODUCT_BUNDLE_PACKAGE_TYPE)</string>
    <key>CFBundleShortVersionString</key>
    <string>1.0.0</string>
    <key>CFBundleVersion</key>
    <string>1</string>
    <key>LSApplicationCategoryType</key>
    <string>public.app-category.utilities</string>
    <key>LSMinimumSystemVersion</key>
    <string>$(MACOSX_DEPLOYMENT_TARGET)</string>
    <key>LSUIElement</key>
    <true/>
    <key>NSHumanReadableCopyright</key>
    <string>Copyright © 2025 MacMount. All rights reserved.</string>
    <key>NSMainStoryboardFile</key>
    <string></string>
    <key>NSPrincipalClass</key>
    <string>NSApplication</string>
    <key>NSSupportsAutomaticTermination</key>
    <false/>
    <key>NSSupportsSuddenTermination</key>
    <false/>
    <key>NSAppleEventsUsageDescription</key>
    <string>MacMount needs to send Apple Events to mount and manage network drives on your system.</string>
    <key>NSSystemAdministrationUsageDescription</key>
    <string>MacMount requires administrator privileges to mount network drives and manage system-level network connections.</string>
    <key>NSNetworkVolumesUsageDescription</key>
    <string>MacMount needs access to network volumes to mount and manage your configured network drives.</string>
    <key>NSRemovableVolumesUsageDescription</key>
    <string>MacMount may need access to removable volumes when mounting certain types of network storage.</string>
    <key>NSDesktopFolderUsageDescription</key>
    <string>MacMount may need access to your Desktop folder to create mount point shortcuts if configured.</string>
    <key>NSDocumentsFolderUsageDescription</key>
    <string>MacMount may need access to your Documents folder to create mount point shortcuts if configured.</string>
    <key>NSDownloadsFolderUsageDescription</key>
    <string>MacMount may need access to your Downloads folder to save diagnostic logs when requested.</string>
    <key>SUEnableAutomaticChecks</key>
    <true/>
    <key>SUFeedURL</key>
    <string>https://macmount.app/appcast.xml</string>
    <key>SUPublicEDKey</key>
    <string>PLACEHOLDER_SPARKLE_PUBLIC_KEY</string>
    <key>CFBundleGetInfoString</key>
    <string>MacMount 1.0.0, Copyright © 2025 MacMount.</string>
    <key>NSHighResolutionCapable</key>
    <true/>
    <key>NSRequiresAquaSystemAppearance</key>
    <false/>
    <key>NSAppleScriptEnabled</key>
    <false/>
    <key>OSAScriptingDefinition</key>
    <string></string>
    <key>UTExportedTypeDeclarations</key>
    <array/>
    <key>UTImportedTypeDeclarations</key>
    <array/>
    <key>NSServices</key>
    <array/>
    <key>CFBundleURLTypes</key>
    <array>
        <dict>
            <key>CFBundleURLName</key>
            <string>MacMount URL</string>
            <key>CFBundleURLSchemes</key>
            <array>
                <string>macmount</string>
            </array>
        </dict>
    </array>
</dict>
</plist>